<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="446.0" prefWidth="515.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="ru.mail.desktopandroidapp.controllers.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Добро пожаловать" />
   <Label text="Войдите в аккаунт" />
   <Label text="Имя " />
   <TextField maxHeight="-Infinity" fx:id="firstname_text" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="264.0" />
   <Label text="Фамилия" />
   <TextField maxHeight="-Infinity" fx:id="lastname_text" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="264.0" />
   <Label text="Пароль" />
   <TextField maxHeight="-Infinity" fx:id="password_text" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="264.0" />
   <Button mnemonicParsing="false" text="Button" onAction="#LogInAccount" />
</VBox>
